{
    "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "timestamp": {
            "@id": "https://dracor.org/ontology_v2#api_response_timestamp",
            "@type": "xsd:dateTime"
        },
        "data": {
            "@id": "https://dracor.org/ontology_v2#contains_data",
            "@context": {
                "name": {
                    "@id": "https://dracor.org/ontology_v2#corpus_name",
                    "@type": "xsd:string"
                },
                "title": {
                    "@id": "https://dracor.org/ontology_v2#corpus_title",
                    "@type": "xsd:string"
                },
                "acronym": {
                    "@id": "https://dracor.org/ontology_v2#corpus_acronym",
                    "@type": "xsd:string"
                },
                "description": {
                    "@id": "https://dracor.org/ontology_v2#corpus_description",
                    "@type": "xsd:string"
                },
                "repository": {
                    "@id": "https://dracor.org/ontology_v2#corpus_repository",
                    "@type": "@id"
                },
                "licence": {
                    "@id": "https://dracor.org/ontology_v2#corpus_licence",
                    "@type": "xsd:string"
                },
                "licenceUrl": {
                    "@id": "https://dracor.org/ontology_v2#corpus_licence_url",
                    "@type": "@id"
                },
