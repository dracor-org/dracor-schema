                        "plays": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_plays",
                            "@type": "xsd:integer"
                        },
                        "characters": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_characters",
                            "@type": "xsd:integer"
                        },
                        "male": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_characters_male",
                            "@type": "xsd:integer"
                        },
                        "female": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_characters_female",
                            "@type": "xsd:integer"
                        },
                        "text": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_tei_text_elements",
                            "@type": "xsd:integer"
