                        "sp": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_sp",
                            "@type": "xsd:integer"
                        },
                        "stage": {
                            "@id": "https://dracor.org/ontology_v2#corpus_num_of_stage",
                            "@type": "xsd:integer"
                        },
                        "wordcount": {
                            "@id": "https://dracor.org/ontology_v2#contains_corpus_wordcount_data",
                            "@context": {
                                "text": {
                                    "@id": "https://dracor.org/ontology_v2#corpus_num_of_word_tokens_in_text_elements",
                                    "@type": "xsd:integer"
                                },
                                "sp": {
                                    "@id": "https://dracor.org/ontology_v2#corpus_num_of_word_tokens_in_sp",
                                    "@type": "xsd:integer"
                                },
                                "stage": {
                                    "@id": "https://dracor.org/ontology_v2#corpus_num_of_word_tokens_in_stage",
                                    "@type": "xsd:integer"
                                }
                            }
                        },
                        "updated": {
                            "@id": "https://dracor.org/ontology_v2#corpus_metrics_date_updated",
                            "@type": "xsd:dateTime"
